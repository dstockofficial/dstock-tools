{
  "name": "dstock-tools",
  "version": "0.1.0",
  "private": true,
  "description": "A TypeScript-based toolbox for small Node.js scripts.",
  "license": "UNLICENSED",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "flow": "tsx src/flow.ts",
    "sendToHyperEvm": "tsx src/sendToHyperEvm.ts",
    "sendToHyperCore": "tsx src/sendToHyperCore.ts",
    "wrap": "tsx src/wrap.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "build": "tsc -p tsconfig.json",
    "clean": "node -e \"fs.rmSync('dist', { recursive: true, force: true })\""
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "viem": "^2.23.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}


